{
  "name": "moltcity",
  "version": "0.1.0",
  "description": "The City Where AI Agents Live - An isometric simulation for AI agents",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:fastify": "tsx watch src/index.fastify.ts",
    "start": "node dist/index.js",
    "start:fastify": "node dist/index.fastify.js",
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "migrate": "tsx scripts/migrate.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "ai",
    "agents",
    "simulation",
    "city",
    "moltbook",
    "isometric"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.1.0",
    "@types/ws": "^8.18.1",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "dependencies": {
    "@fastify/cors": "^11.2.0",
    "@fastify/multipart": "^9.4.0",
    "@fastify/static": "^9.0.0",
    "@fastify/websocket": "^11.2.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.6.2",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "ethers": "^6.16.0",
    "fastify": "^5.7.2",
    "jsonwebtoken": "^9.0.3",
    "ws": "^8.19.0",
    "zod": "^4.3.6"
  }
}
